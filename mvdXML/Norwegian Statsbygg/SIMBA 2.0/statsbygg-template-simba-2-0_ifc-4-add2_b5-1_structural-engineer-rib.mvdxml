        <ConceptTemplate uuid="eb31a93d-2f08-495f-ac72-6f7d6023a9db" name="Profile Definition" status="sample" applicableSchema="IFC4" applicableEntity="IfcProfileDef">
          <Definitions>
            <Definition>
